{
  "name": "mmarseglia-packer",
  "version": "2.0.0",
  "source": "git://github.com/mmarseglia/puppet-packer.git",
  "author": "Mike Marseglia",
  "license": "Apache-2.0",
  "summary": "Puppet module for installing Packer, the modern automated machine image creation tool.",
  "project_page": "https://github.com/mmarseglia/puppet-packer",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement":">= 2.4.0 < 5.0.0"
    },
    {
      "name": "puppet/archive",
      "version_requirement": ">= 1.0.0 < 3.0.0"
    }
  ],
  "operatingsystem_support": [
      {
          "operatingsystem": "CentOS"
      },
      {
          "operatingsystem": "Darwin"
      },
      {
          "operatingsystem": "Debian"
      },
      {
          "operatingsystem": "OpenBSD"
      },
      {
          "operatingsystem": "RedHat"
      },
      {
          "operatingsystem": "Ubuntu"
      }
  ]
}
